{
  "manifest_version": 3,
  "name": "Skroutz Vault",
  "version": "1.0.0",
  "description": "Search for archived products on Skroutz",
  "permissions": [],
  "host_permissions": [
    "https://*.skroutz.gr/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Skroutz Vault"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
